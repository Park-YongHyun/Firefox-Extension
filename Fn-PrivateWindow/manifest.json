{

	"manifest_version": 2,
	"name": "Fn-PrivateWindow",
	"version": "1.0",
	"description": "",

	"applications": {
		"gecko": {
			"id": "PAddon@Fn-PrivateWindow"
			}
	},

	"permissions": [
		"activeTab",
		"menus",
		"tabs",
		"<all_urls>"
	],

	"icons": {
		"48": "icons/icon.svg",
		"96": "icons/icon.svg"
	},

	"content_scripts": [
		{
			"all_frames": true,
			"match_about_blank": true,
			"matches": ["<all_urls>"],
			"js": ["script-content.js"]
		}
	],

	"background": {
		"scripts": ["script-background.js"]
	}

}